name:                  mismi-s3-test
version:               0.0.1
cabal-version:         >= 1.8
build-type:            Simple

library
  build-depends:
                       base                            >= 3          && < 5
                     , mismi-core
                     , mismi-core-test
                     , mismi-s3
                     , amazonka                        == 99.0.9
                     , amazonka-core                   == 99.0.9
                     , amazonka-s3                     == 99.0.9
                     , lens                            >= 4.8        && < 4.10
                     , conduit                         == 1.2.*
                     , time                            == 1.4.*
                     , disorder-core
                     , disorder-corpus
                     , either                          == 4.3.*
                     , exceptions                      >= 0.6        && < 0.9
                     , filepath                        == 1.3.*
                     , p
                     , http-client                     == 0.4.18.*
                     , text                            == 1.2.*
                     , transformers                    >= 0.3        && < 0.5
                     , QuickCheck                      == 2.7.*
                     , quickcheck-instances            == 0.3.*
                     , unix                            == 2.7.*
                     , temporary                       == 1.2.*
                     , uuid                            == 1.3.*

  exposed-modules:
                     Test.Mismi.Amazonka
                     Test.Mismi.S3
                     Test.Mismi.S3.Arbitrary
